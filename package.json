    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@types/http-errors": "^1.8.2",
    "@types/joi-phone-number": "^5.0.3",
    "@types/qrcode": "^1.4.2",
    "@types/randomstring": "^1.1.8",
    "@types/redis": "^4.0.11",
    "await-to-js": "^3.0.0",
    "aws-sdk": "^2.1033.0",
    "buffer-to-data-url": "^1.0.0",
    "dot-env": "^0.0.1",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
